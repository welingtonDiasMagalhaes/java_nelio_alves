package _14_metodosAbstratos.entities;

public abstract class Shape {
	public Color Color;

	public Shape() {
		super();
	}

	public Shape(String color) {
		super();
		Color = color;
	}

	public String getColor() {
		return Color;
	}

	public void setColor(String color) {
		Color = color;
	}
	
	public void abstract area() {		
	}
	
}
