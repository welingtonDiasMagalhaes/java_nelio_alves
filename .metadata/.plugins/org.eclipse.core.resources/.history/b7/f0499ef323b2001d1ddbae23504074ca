package _14_metodosAbstratos.entities;

public abstract class Shape {
	public Color Color;

	public Shape() {
		super();
	}

	public Shape(Color color) {
		super();
		Color = color;
	}

	public Color getColor() {
		return Color;
	}

	public void setColor(Color color) {
		Color = color;
	}
	
	public abstract double area();
	
}
